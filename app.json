{
  "expo": {
    "name": "Zerodech",
    "slug": "Zerodech-1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/image2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
    "supportsTablet": true,
    "splash":{
    "image": "./assets/images/image2.png",
    "backgroundColor": "#ffffff",
    "resizeMode":"contain"
    }
    },

    "android": {
    "supportsTablet": true,
    "image": "./assets/images/image2.png",
    "backgroundColor": "#ffffff",
    "resizeMode":"contain",
    "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/image1",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
